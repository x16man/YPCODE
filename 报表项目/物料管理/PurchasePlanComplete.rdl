  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>select a.*,b.inNum,b.inprice,b.inmoney
from
(select ppln.entrydate,pdop.*,witm.NewCode
from pdop,ppln,witm 
where ppln.entryno=pdop.entryno and 
      ppln.entryno = @EntryNo and 
      pdop.itemnum &gt; 0 and
      pdop.ItemCode=WITM.Code) As A
Left Outer Join
(select a.*,pdob.itemnum as innum,pdob.itemprice as inprice,pdob.itemsum as inmoney
from 
(select * from pdor where sourceentry =@EntryNo and sourcedoccode=5) As a,
pdob,pbor
where pbor.entryState = 'I' and
      pbor.entryno = pdob.entryno and
      pdob.sourceentry = a.entryno and
      pdob.sourcedoccode = 3 and
      pdob.sourceserialno = a.serialno) as B
on a.entryno = b.sourceEntry and 
   a.serialno = b.sourceserialno</CommandText>
        <QueryParameters>
          <QueryParameter Name="@EntryNo">
            <Value>=Parameters!EntryNo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>mmdb</DataSourceName>
      </Query>
