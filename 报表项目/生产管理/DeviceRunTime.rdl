        <Field Name="I_TAG_ID">
          <DataField>I_TAG_ID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="REMARK">
          <DataField>REMARK</DataField>
          <rd:TypeName>System.String</rd:TypeName>
