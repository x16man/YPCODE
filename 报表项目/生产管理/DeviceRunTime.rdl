        <Field Name="RUN_TIME">
          <DataField>RUN_TIME</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>gather</DataSourceName>
        <CommandText>SELECT B.PKID,A.DEV_CODE,A.DEV_NAME,A.DEV_MODEL,A.DEV_SPEC,A.USE_DEP,
       B.STATUS,B.I_TAG_ID,B.REMARK,
       ROUND(SUM(CONVERT(FLOAT,C.END_TIME - C.BEGIN_TIME))*24,1) RUN_TIME 
FROM   D_DEV_ACCOUNT A  INner JOIN D_RUN_STATUS B 
ON     A.DEV_CODE = B.DEV_CODE AND 
       B.END_TIME IS NULL  
LEFT JOIN D_RUN_STATUS C 
ON 	A.DEV_CODE = C.DEV_CODE AND 
   	C.BEGIN_TIME &gt;= @BeginTime AND 
        C.Begin_Time &lt; @EndTime AND
   	C.STATUS = 1 
WHERE A.RECORD_RUN_STATUS = 1 AND 
      A.STATE &lt;&gt;8  and 
      A.TYPE_CODE like '01%'  
GROUP BY B.PKID,A.DEV_CODE,A.DEV_NAME,A.DEV_MODEL,A.DEV_SPEC,A.USE_DEP,B.STATUS,B.I_Tag_ID,B.remark  
ORDER BY use_dep,A.Dev_Code</CommandText>
        <QueryParameters>
          <QueryParameter Name="@BeginTime">
            <Value>=Parameters!BeginTime.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndTime">
            <Value>=Parameters!EndTime.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
