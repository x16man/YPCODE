  </DataSources>
  <Width>23.25cm</Width>
  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="Year">
          <DataField>Year</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="WValue">
          <DataField>WValue</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="EValue">
          <DataField>EValue</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="HValue">
          <DataField>HValue</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="CValue">
          <DataField>CValue</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="AValue">
          <DataField>AValue</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="YValue">
          <DataField>YValue</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="GValue">
          <DataField>GValue</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>gather</DataSourceName>
        <CommandText>Select 	A.Year,
	A.I_Value_Man as WValue,
	B.I_Value_Man as EValue,
	C.I_Value_Man As HValue,
	F.I_Value_Man As CValue,
	D.I_Value_Man As AValue,
	E.I_Value_Man As YValue,
        G.I_Value_Man As GValue
From
(Select Year(dbo.DayCycleId2DateTime(i_cycle_ID)) AS Year,Sum(I_Value_Man) as I_Value_man 
From T_Tag_Day 
Where I_Tag_Id = '5003001' And I_Value_man &gt; 0 And I_Cycle_Id&gt;=20050101
Group By Year(dbo.DayCycleId2DateTime(i_cycle_ID))
) AS A 
LEFT OUTER JOIN
(Select Year(dbo.DayCycleId2DateTime(i_cycle_ID)) As Year,Sum(I_Value_Man) as I_Value_man 
From T_Tag_Day 
Where I_Tag_Id = '1803011' And I_Value_Man &gt; 0 And I_Cycle_Id&gt;=20050101
Group By Year(dbo.DayCycleId2DateTime(i_cycle_ID))
) AS B
ON A.Year = B.Year
Left Outer Join
(Select Year(dbo.DayCycleId2DateTime(i_cycle_ID)) As Year,Sum(I_Value_Man) as I_Value_man 
From T_Tag_Day 
Where I_Tag_Id = 'scrbd24' And I_Value_Man &gt; 0 And I_Cycle_Id&gt;=20050101
Group By Year(dbo.DayCycleId2DateTime(i_cycle_ID))
) AS C
ON A.Year = C.Year
Left Outer Join
(Select Year(dbo.DayCycleId2DateTime(i_cycle_ID)) As Year,Sum(I_Value_Man) as I_Value_man 
From T_Tag_Day 
Where I_Tag_Id = '5204001' And I_Value_Man &gt; 0 And I_Cycle_Id&gt;=20050101
Group By Year(dbo.DayCycleId2DateTime(i_cycle_ID))
) as D
ON A.Year = D.Year
Left Outer Join
(Select Year(dbo.DayCycleId2DateTime(i_cycle_ID)) As Year,Sum(I_Value_Man) as I_Value_man 
From T_Tag_Day 
Where I_Tag_Id = '1131001' And I_Value_Man &gt; 0 And I_Cycle_Id&gt;=20050101
Group By Year(dbo.DayCycleId2DateTime(i_cycle_ID))) As E
ON A.Year = E.Year
Left Outer Join
(Select Year(dbo.DayCycleId2DateTime(i_cycle_ID)) As Year,Sum(I_Value_Man) as I_Value_man 
From T_Tag_Day 
Where I_Tag_Id In( '1132030','1132031','1132032','1132033','1132034','1132035','1132036','1132038','1132037','1132040','1132041',
'1132039','1132004','1132005','1132001','1132002','1132003','1132042','1132043',
--'1102001','1102002','1102003','1102004','1102005','1102006','1102007','1102008','1102009','1102010','1102011','1102012','1102013',
--'1102014','1102015','1102016','1102017','1102018','1102019','1102020','1102021','1102022',
--'1104001','1104002','1104003','1104004','1104005','1104006','1104007','1104008','1104009','1104010','1104011','1104012'
'scrbd31','scrbd32') And 
I_Value_Man &gt; 0 And I_Cycle_Id&gt;=20050101
Group By Year(dbo.DayCycleId2DateTime(i_cycle_ID)) ) As F
On A.Year = F.Year
Left Outer Join
(Select Year(dbo.DayCycleId2DateTime(i_cycle_ID)) AS Year,Sum(I_Value_Man) as I_Value_man 
From T_Tag_Day 
Where I_Tag_Id In ( '5001001','5002001') And I_Value_man &gt; 0 And I_Cycle_Id&gt;=20050101
Group By Year(dbo.DayCycleId2DateTime(i_cycle_ID))) As G
On A.Year = G.Year
ORDER By A.Year</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
